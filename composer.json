{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "aws/aws-sdk-php-laravel": "~3.0",
        "guzzlehttp/guzzle": "~6.0",
        "davispeixoto/laravel5-salesforce": "~2.0",
        "markwilson/xml-to-json": "^0.1.4",
        "michaelb/lumen-make": "^1.0",
        "laravelista/lumen-vendor-publish": "^2.0",
        "jem-get-qualified/kodosunifa": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": ["app/Support/Helpers.php"]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-install-cmd":[
            "php vendor/jem-get-qualified/kodosunifa/script/install-hooks.php"
        ],
        "post-update-cmd":[
            "php vendor/jem-get-qualified/kodosunifa/script/install-hooks.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
